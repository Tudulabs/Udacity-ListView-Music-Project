<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.mydailyworkouts.musicalstructureproject3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SongListing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DrakesProfile"
            android:label="@string/drakes_profile"
            android:parentActivityName=".SongListing"/>
        <activity android:name=".SelenaGomezProfile"
            android:label="@string/selena_gomez"
            android:parentActivityName=".SongListing"/>
        <activity android:name=".BeyonceProfile"
            android:label="@string/beyonce"
            android:parentActivityName=".SongListing"/>
        <activity android:name=".Blink182Profile"
            android:label="@string/blink_182"
            android:parentActivityName=".SongListing"></activity>
    </application>

</manifest>